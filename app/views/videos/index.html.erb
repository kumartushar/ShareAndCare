<div class="blog-header-view bg_cadetblue">
  <div id="mytube-header" class="blog-header padding-15">
    <div class="logo">MyTube</div>
    <input class="form-control mytube-search" id="mytube-search-field" type="text" placeholder="What are you looking for">
    <span class="glyphicon glyphicon-search"></span>
    <div class="mytube-actions">
      <span class="glyphicon glyphicon-upload"></span>
    </div>
  </div>
</div>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= video.title %></td>
        <td><%= video.url %></td>
        <td><%= link_to 'Show', video %></td>
        <td><%= link_to 'Edit', edit_video_path(video) %></td>
        <td><%= link_to 'Destroy', video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video', new_video_path %>
<%= javascript_include_tag "video_index" %>